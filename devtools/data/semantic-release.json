{
  "branches": [
    "main",
    {
      "name": "dev",
      "prerelease": "rc"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    [
      "@semantic-release/exec",
      {
        "verifyReleaseCmd": "devtools-node print-next-version-helper \"${nextRelease.version}\""
      }
    ]
  ]
}
